<?xml version="1.0" encoding="utf-8"?>
<com.jwenfeng.library.pulltorefresh.PullToRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_contribute_img_pulltorefresh_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.codekong.imageclassificationsystemclient.activity.ContributeImgActivity">

    <ListView
        android:id="@+id/id_contribute_img_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="#afaea8"
        android:dividerHeight="1dp">


    </ListView>
    <FrameLayout
        android:id="@+id/id_progress_framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">
        <ProgressBar
            android:id="@+id/id_loading_progressbar"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"/>
    </FrameLayout>
</com.jwenfeng.library.pulltorefresh.PullToRefreshLayout>
